* {\n  box-sizing: border-box;\n  margin: 0;\n  padding: 0;\n  font-family: $serif;\n}\n\n//MIXIN\n\n@mixin main-text {\n  color: $text;\n  letter-spacing: 0.1rem;\n  text-align: $center;\n}\n\n@mixin paragraph-text {\n  color: $blue-gray;\n  font-family: $serif;\n  font-size: $font-s;\n}\n\n@mixin quote-text {\n  color: $blue-gray;\n  font-family: $quote;\n  font-size: $font-xs;\n  text-align: $center;\n  font-style: italic;\n}\n\n@mixin input-text {\n  color: $secondary;\n  font-family: $sans-serif;\n  font-size: $font-s;\n}\n\n@mixin flex {\n  display: flex;\n}\n\n//TYPOGRAPHY\n\nh1 {\n  @include main-text;\n  font-weight: $extra-bold;\n  font-size: $font-l;\n}\n\nh2 {\n  @include main-text;\n  font-weight: $bold;\n  font-size: $font-m;\n}\n\nh3 {\n  @include main-text;\n  text-align: $left;\n  font-weight: normal;\n}\n\nh4 {\n  @include quote-text;\n}\nh6 {\n  @include input-text;\n}\n\np {\n  @include paragraph-text;\n}\n"},"lineCount":null}},"error":null,"hash":"158f22e8ffd9d5089d29e30d3909583c","cacheData":{"env":{}}}