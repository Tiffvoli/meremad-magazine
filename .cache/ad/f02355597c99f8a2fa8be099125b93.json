ng-quote: 0.5rem 0 0 2rem;\n// $margin-quote: 0.5rem 0 0 2rem;\n// $margin-send: 0.4rem 0rem 0 1rem;\n\n//WIDTH & HEIGHT\n\n$max-width: 100%;\n\n//GRID\n\n$grid: grid;\n$hidden: hidden;\n$auto: auto;\n$grid-column-l: 2fr 1fr;\n$grid-column-m: repeat(4, 7rem);\n$grid-column-s: repeat(2, 1fr);\n","styles/_base.scss":"* {\n  box-sizing: border-box;\n  margin: 0;\n  padding: 0;\n  font-family: $serif;\n}\n\n//MIXIN\n\n@mixin main-text {\n  color: $text;\n  letter-spacing: 0.1rem;\n  text-align: $center;\n}\n\n@mixin paragraph-text {\n  color: $blue-gray;\n  font-family: $sans-serif;\n  font-size: $font-s;\n}\n\n@mixin quote-text {\n  color: $blue-gray;\n  font-family: $quote;\n  font-size: $font-xs;\n  text-align: $center;\n  font-style: italic;\n}\n\n@mixin input-text {\n  color: $secondary;\n  font-family: $sans-serif;\n  font-size: $font-s;\n}\n\n@mixin categories {\n  font-family: $quote;\n  font-size: 1rem;\n  color: $text;\n  text-decoration: none;\n  text-align: center;\n\n  &:hover {\n    color: $blush;\n  }\n}\n\n@mixin flex {\n  display: flex;\n}\n\n@mixin padding-xl {\n  padding: $padding-xl;\n\n  @media (max-width: $max-m) {\n    padding: $padding-l;\n  }\n}\n\n@mixin grid {\n  display: grid;\n  grid-template-columns: repeat(auto-fit, 23rem);\n  grid-column-gap: 1rem;\n  grid-row-gap: 4rem;\n  justify-content: center;\n}\n\n//TYPOGRAPHY\n\nh1 {\n  @include main-text;\n  font-weight: $extra-bold;\n  font-size: $font-l;\n  padding-bottom: 1rem;\n}\n\nh2 {\n  @include main-text;\n  font-weight: normal;\n  font-size: $font-m;\n  padding-bottom: 1rem;\n}\n\nh3 {\n  @include main-text;\n  text-align: $left;\n  font-weight: normal;\n}\n\nh4 {\n  @include quote-text;\n}\nh5 {\n  @include main-text;\n  font-weight: normal;\n  font-size: $font-s;\n  padding: 1.5rem 0;\n  border: $border-black;\n  position: absolute;\n  bottom: 0;\n  width: 100%;\n  background-color: $white;\n\n  &:hover {\n    color: $blush;\n  }\n}\nh6 {\n  @include input-text;\n}\n\np {\n  @include paragraph-text;\n}\n\n.highlight {\n  color: $blush !important;\n}\n","styles/style.scss":"@import \"variables\";\n@import \"base\";\n\n//BANNER\n\n//BANNER\n\n.banner {\n  height: 50vh;\n  background: $secondary;\n  width: 100%;\n\n  @media (max-width: $max-l) {\n    height: 30vh;\n  }\n}\n\n.padding-xl {\n  @include padding-xl;\n}\n\n.banner-img {\n  width: 100%;\n  height: 100%;\n  object-fit: cover;\n}\n\n//SUB BANNER\n.categories {\n  display: flex;\n  justify-content: space-around;\n  margin: 0.5rem 0;\n  border-color: $blue-gray;\n  border-style: none none none solid;\n  border-width: 1px;\n\n  & a {\n    width: 17.5%;\n    @include categories;\n    border-color: $blue-gray;\n    border-style: none solid none none;\n    border-width: 1px;\n  }\n}\n\n//SORTING\n.filter-sort {\n  padding: 1rem 2rem;\n}\n\n.filter-sort {\n  @include paragraph-text;\n  font-size: $font-xs;\n  display: block;\n  position: relative;\n}\n.filter-sort-list {\n  @include paragraph-text;\n  font-size: $font-xs;\n  padding: 0 1rem;\n\n  & li {\n    list-style: none;\n    padding: 0.1rem;\n  }\n\n  & label {\n    @extend .filter-sort-list;\n    padding-left: 0.5rem;\n  }\n}\n.dropdown {\n  visibility: hidden;\n  opacity: 0;\n  background: $white;\n  position: absolute;\n  padding: 1rem;\n  left: 2rem;\n  display: none;\n  border: $border-black;\n  z-index: 1;\n}\n\n.filter-sort:hover > .dropdown,\n.filter-sort .dropdown:hover {\n  visibility: visible;\n  opacity: 1;\n  @include flex;\n}\n\n//CATALOGUE\n.catalogue-grid {\n  @include grid;\n}\n\n.catalogue {\n  display: flex;\n  flex-direction: column;\n  overflow: hidden;\n  height: 21rem;\n  position: relative;\n\n  &-info {\n    background: rgba(255, 255, 255, 0.8);\n    bottom: -5.55rem;\n    position: absolute;\n    transition: all 0.3s ease-out;\n    display: flex;\n    flex-direction: column;\n\n    & h5 {\n      position: relative;\n    }\n  }\n  & img {\n    width: $max-width;\n    height: 100%;\n    object-fit: cover;\n    position: relative;\n  }\n\n  & p {\n    font-size: 0.8rem;\n    line-height: 1.2rem;\n    padding: 1rem;\n    flex-grow: 1;\n  }\n}\n.catalogue:hover > .catalogue-info {\n  transform: translate3d(0, -5.55rem, 0);\n  border: none;\n\n  & h5 {\n    border: none;\n    background: transparent;\n  }\n}\n\n.pagination {\n  justify-content: $center;\n  display: flex;\n  padding: 1rem 0;\n\n  & .dot {\n    height: 1rem;\n    width: 1rem;\n    background-color: $white;\n    border: $border-black;\n    border-radius: 50%;\n    margin: 0 0.2rem;\n\n    &-active,\n    &:hover {\n      background-color: $blush;\n    }\n  }\n}\n"},"lineCount":null}},"error":null,"hash":"9f4378269332a470d0b7c7ef765dc312","cacheData":{"env":{}}}