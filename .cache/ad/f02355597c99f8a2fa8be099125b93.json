 font-size: 1rem;\n  color: $text;\n  text-decoration: none;\n  text-align: center;\n\n  &:hover {\n    color: $blush;\n  }\n}\n\n@mixin flex {\n  display: flex;\n}\n@mixin flex-column {\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n}\n\n@mixin padding-xl {\n  padding: $padding-xl;\n\n  @media (max-width: $max-m) {\n    padding: $padding-l;\n  }\n}\n\n@mixin grid {\n  display: grid;\n  grid-template-columns: repeat(auto-fit, 22.8rem);\n  grid-column-gap: 1rem;\n  grid-row-gap: 4rem;\n  justify-content: center;\n}\n\n//TYPOGRAPHY\n\nh1 {\n  @include main-text;\n  font-weight: $extra-bold;\n  font-size: $font-l;\n  padding-bottom: 1rem;\n}\n\nh2 {\n  @include main-text;\n  font-weight: $bold;\n  font-size: $font-m;\n  padding-bottom: 1rem;\n}\n\nh3 {\n  @include main-text;\n  text-align: $left;\n  font-weight: normal;\n  letter-spacing: 0.05rem;\n}\n\nh4 {\n  @include quote-text;\n  padding-bottom: 3rem;\n}\nh5 {\n  @include main-text;\n  font-weight: normal;\n  font-size: $font-s;\n  padding: 1.5rem 0;\n  border: $border-black;\n  position: absolute;\n  bottom: 0;\n  width: 100%;\n  background-color: $white;\n\n  &:hover {\n    color: $blush;\n  }\n}\nh6 {\n  @include input-text;\n}\n\np {\n  @include paragraph-text;\n}\n\n.highlight {\n  color: $blush !important;\n}\n.link {\n  @include paragraph-text;\n  color: $secondary;\n\n  &-button {\n    @include paragraph-text;\n    font-size: 1rem;\n    text-decoration: none;\n    border: $border;\n    padding: 0.5em 1rem;\n    width: fit-content;\n    cursor: pointer;\n\n    &:hover {\n      background: $blush;\n    }\n    & a {\n      all: unset;\n      color: $primary;\n    }\n  }\n}\n.page-heading {\n  @include main-text;\n  letter-spacing: 0.05rem;\n  font-weight: bold;\n  font-size: 2.6rem;\n  padding: 1rem 0 0.2rem;\n}\n.subheading {\n  @include paragraph-text;\n  font-size: 1rem;\n  text-align: $center;\n}\n.text-center {\n  text-align: center;\n}\n.text-right {\n  text-align: right;\n}\n.text-left {\n  text-align: left;\n}\n.text-bold {\n  font-weight: bold !important;\n}\n.text-italic {\n  font-style: italic;\n}\n.padding-left {\n  padding-left: 1rem;\n}\n.padding-right {\n  padding-right: 1rem;\n}\n.flex {\n  @include flex;\n}\n"},"lineCount":null}},"error":null,"hash":"b2af4e68e4e557de2f0bfcf3a823d4b1","cacheData":{"env":{}}}